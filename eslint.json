{  "extends": [    "airbnb",    "plugin:react/recommended",    "airbnb",    "airbnb/hooks"  ],  "rules": {    "prettier/prettier": ["error"],    "react/prop-types": 0,    "react/jsx-one-expression-per-line": 0,    "react/jsx-filename-extension": 0,    "no-use-before-define": 1,    "no-unused-vars": 2,    "no-console": "warn",    "quotes": ["error", "single"]  },  "settings": {    "import/resolver": {      "node": {        "paths": ["."]      }    }  }}